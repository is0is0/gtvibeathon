{
  "exclude": [
    "**/__pycache__",
    "**/*.pyc",
    "**/*.pyo",
    "**/*.pyd",
    "**/.Python",
    "**/env",
    "**/venv",
    "**/.venv",
    "**/ENV",
    "**/env.bak",
    "**/venv.bak",
    "**/.pytest_cache",
    "**/.coverage",
    "**/htmlcov",
    "**/.tox",
    "**/.nox",
    "**/coverage.xml",
    "**/*.cover",
    "**/*.log",
    "**/logs",
    "**/node_modules",
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/*.tmp",
    "**/*.temp",
    "**/tmp",
    "**/temp",
    "**/build",
    "**/dist",
    "**/.eggs",
    "**/*.egg-info",
    "**/.mypy_cache",
    "**/.ruff_cache",
    "**/.black_cache"
  ],
  "ignore": [
    "src/voxel/agents/shader/train.py",
    "src/voxel/agents/texture/train.py"
  ],
  "include": [
    "src/**/*.py",
    "tests/**/*.py",
    "addons/**/*.py",
    "*.py"
  ],
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "Darwin"
}
